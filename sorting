#include<bits/stdc++.h>
using namespace std;
int main()
{
    int array[]={3,1,78,54,6,1};
    int n=sizeof(array)/sizeof(array[0]);
    for(int i=0;i<n-1;i++)
    {
        int swaped=0;
        for(int j=0;j<n-i-1;j++)
        {
           if(array[j]>array[j+1])
           {
              swap(array[j],array[j+1]);
              swaped++;
            }
          }
          if(swaped==0)
               break;
      
        
    }
    
    for(auto it:array)
    {
        cout<<it<<endl;
    }
    
}
